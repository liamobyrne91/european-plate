   "import":[
      {
         "name":"alexa-layouts",
         "version":"1.7.0"
      }
   ],
   "mainTemplate":{
      "parameters":[
         "payload"
      ],
      "items":[
         {
            "type":"AlexaDetail",
            "detailImageAlignment":"right",
            "headerTitle":"${payload.detailImageRightData.title}",
            "headerAttributionImage":"${payload.detailImageRightData.logoUrl}",
            "headerDivider":true,
            "hintText": "Hint Text",
            "imageBlurredBackground":false,
            "imageScale":"best-fit",
            "imageAspectRatio":"square",
            "imageAlignment":"right",
            "imageShadow":false,
            "imageSource":"${payload.detailImageRightData.detailImage}",
            "primaryText":"${payload.detailImageRightData.primaryText}",
            "backgroundImageSource":"",
            "theme":"dark"
         }
      ]
   }
}
